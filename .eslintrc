{
  "extends": "airbnb",
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "class-methods-use-this": 0,
    "no-console": 0,
    "no-param-reassign": 0,
    "comma-dangle": ["error", {
      "arrays": "never",
      "objects": "only-multiline",
      "imports": "never",
      "exports": "never",
      "functions": "ignore"
    }]
  }
}